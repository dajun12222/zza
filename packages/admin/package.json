{
  "name": "cloudpivot-admin",
  "version": "6.13.0",
  "private": true,
  "description": "云枢admin端项目",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "npm run build-prod",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "analyze": "vue-cli-service build --report",
    "build-dev": "vue-cli-service build --mode development",
    "build-prod": "rimraf node_modules/.cache && vue-cli-service build --max_old_space_size=8192",
    "dev": "rimraf node_modules/.cache && cross-env node --max-http-header-size=1024000 --max_old_space_size=8192 node_modules/@vue/cli-service/bin/vue-cli-service.js serve --mode debug",
    "dll": "webpack -p --progress --config ./webpack.dll.conf.js",
    "local": "rimraf node_modules/.cache && cross-env node --max_old_space_size=8192 node_modules/@vue/cli-service/bin/vue-cli-service.js serve --mode local",
    "test": "vue-cli-service test:unit"
  },
  "dependencies": {
    "canvg": "1.5.3",
    "cloudpivot": "6.13.1000",
    "cloudpivot-admin-core": "6.13.0",
    "cloudpivot-designer": "6.13.0",
    "cloudpivot-flow": "6.13.1003",
    "cloudpivot-form": "6.13.1001",
    "cloudpivot-forms": "6.13.0",
    "cloudpivot-icons": "6.13.0",
    "cloudpivot-kindeditor": "6.11.1000",
    "cloudpivot-list": "6.13.0",
    "cloudpivot-gantt": "6.12.2",
    "cloudpivot-platform": "6.13.0",
    "cloudpivot-portal-designer": "6.13.1002",
    "extension-template": "6.13.0",
    "html2canvas": "1.0.0-rc.3",
    "monaco-editor": "0.17.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.8.4",
    "@vue/cli-plugin-babel": "~4.5.15",
    "@vue/cli-plugin-eslint": "~4.5.15",
    "@vue/cli-plugin-router": "~4.5.15",
    "@vue/cli-plugin-typescript": "~4.5.15",
    "@vue/cli-plugin-vuex": "~4.5.15",
    "@vue/cli-service": "~4.5.15",
    "@vue/eslint-config-typescript": "^7.0.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.33",
    "babel-plugin-import": "^1.13.3",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-vue-jsx": "^3.7.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "less": "^3.0.4",
    "less-loader": "^5.0.0",
    "monaco-editor-webpack-plugin": "1.7.0",
    "typescript": "~4.1.5",
    "uuidjs": "^4.2.8",
    "vue-template-compiler": "2.6.14"
  }
}
