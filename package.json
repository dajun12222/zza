{
  "name": "fronted",
  "version": "6.13.0",
  "private": true,
  "description": "云枢6.13多包项目工程",
  "scripts": {
    "cz": "yarn run addNotes && npx git-cz",
    "dev:mobile": "cd packages/mobile && yarn run serve",
    "dev:portal": "cd packages/portal && yarn run serve",
    "dev:admin": "cd packages/admin && yarn run serve",
    "build:admin": "cd packages/admin && yarn run build",
    "build:portal": "cd packages/portal && yarn run build",
